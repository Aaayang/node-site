<ol class="breadcrumb">
    <li><a href="/admin">后台</a></li>
    <li class="active">用户列表</li>
</ol>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>#</th>
            <th>ID</th>
            <th>用户名</th>
            <th>密码</th>
            <th>是否是管理员</th>
        </tr>
    </thead>
    <tbody>
        <% userList.forEach((user,idx) => { %>
            <tr>
                <th scope="row"><%= idx %></th>
                <td><%= user._id %></td>
                <td><%= user.username %></td>
                <td><%= user.password %></td>
                <td><%= user.isAdmin ? '是' : '否' %></td>
            </tr>
        <% }); %>
    </tbody>
</table>

<% include ../common/navigation %>